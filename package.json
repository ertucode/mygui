{
  "name": "mygui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron; cross-env NODE_ENV=development electron .",
    "build:react": "tsc -b && vite build",
    "check:frontend": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc -p ./src/electron/tsconfig.json",
    "build": "npm run transpile:electron && npm run build:react && electron-builder --mac --arm64",
    "build-vendor": "node scripts/build-all.mjs",
    "build-everything": "npm run build-vendor && npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.17",
    "@types/adm-zip": "^0.5.7",
    "@use-gesture/react": "^10.3.1",
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.13.0",
    "adm-zip": "^0.5.16",
    "clipboardy": "^5.0.1",
    "csv-parse": "^6.1.0",
    "daisyui": "^5.5.5",
    "docx-preview": "^0.3.7",
    "flexlayout-react": "^0.8.17",
    "fuse.js": "^7.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.554.0",
    "os-utils": "^0.0.14",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.68.0",
    "react-use-measure": "^2.1.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "xlsx": "^0.18.5",
    "xstate": "^5.25.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
