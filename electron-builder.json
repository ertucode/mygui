{
  "appId": "com.ertucode.mygui",
  "files": ["dist-electron", "dist-react", "assets"],
  "extraResources": [
    "dist-electron/electron/preload.cjs",
    "dist-electron/electron/preload-preview.cjs",
    {
      "from": "vendor-bin",
      "to": "vendor-bin",
      "filter": ["*"]
    }
  ],
  "mac": {
    "target": ["dmg"]
  },
  "linux": {
    "target": ["AppImage"],
    "category": "Utility"
  },
  "win": {
    "target": ["portable", "msi"]
  }
}
